<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jxust.qq.superquestionlib.dao.mapper.admin.EsUserMapper">

    <insert id="createUser" useGeneratedKeys="true" keyProperty="userId" parameterType="com.jxust.qq.superquestionlib.dto.admin.EsUser">
      insert into sq_user(user_name,user_password,user_avatar,user_nick,user_last_login_time,user_create_time,user_sex,user_school_id)
                            value(#{user.userName},#{user.userPassword}
                            ,#{user.userAvatar},#{user.userNick}
                            ,#{user.userLastLoginTime},#{user.userCreateTime},#{user.userSex},#{user.userSchoolId})
    </insert>
    <update id="updateUser">
        update sq_user set user_id=#{user.userId},user_name=#{user.userName},user_password=#{user.userPassword}
        ,user_avatar=#{user.userAvatar},user_nick=#{user.userNick},user_last_login_time=#{user.userLastLoginTime},user_create_time=#{user.userCreateTime}
        ,user_sex=#{user.userSex},user_school_id=#{user.userSchoolId} where user_id=#{user.userId}
    </update>
    <delete id="deleteById">
        delete from sq_user where user_id=#{id}
    </delete>
</mapper>